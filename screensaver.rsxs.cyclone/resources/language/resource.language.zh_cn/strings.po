#. Main category name on addon settings dialog
msgctxt "#30000"
msgid "General"
msgstr "常规"

#. To select amount of rendered Cyclones as number
msgctxt "#30001"
msgid "Cyclones"
msgstr "旋风"

#. Help text for selection of rendered Cyclones
msgctxt "#30002"
msgid "Number of cyclones.  1 - 10.  Default: 1"
msgstr "旋风数。1 - 10。 默认：1"

#. Selection of used particles as number
msgctxt "#30003"
msgid "Particles"
msgstr "粒子"

#. Help text for selection of used particles
msgctxt "#30004"
msgid "Particles per cyclone.  1 - 10000.  Default: 400"
msgstr "每个旋风粒子数。1 - 10000。默认：400"

#. Selection of size for particle as number
msgctxt "#30005"
msgid "Size"
msgstr "大小"

#. Help text for selection of size for particle
msgctxt "#30006"
msgid "Particle size.  1 - 100.  Default: 7"
msgstr "粒子大小。1 - 100。默认：7"

#. Selection of complexity as number
msgctxt "#30007"
msgid "Complexity"
msgstr "复杂度"

#. Help text for selection of complexity
msgctxt "#30008"
msgid "Cyclone complexity.  1 - 10.  Default: 3"
msgstr "旋风复杂度。1 - 10。默认：3"

#. Selection of render speed as number
msgctxt "#30009"
msgid "Speed"
msgstr "速度"

#. Help text for selection of speed
msgctxt "#30010"
msgid "Speed.  1 - 100.  Default: 10"
msgstr "速度。1 - 100。默认：10"

#. Boolean selection to enable stretch
msgctxt "#30011"
msgid "Stretch"
msgstr "拉伸"

#. Help text for selection boolean for stretch
msgctxt "#30012"
msgid "Stretch.  Default: True"
msgstr "拉伸。默认：真"

#. Boolean selection to show of curves
msgctxt "#30013"
msgid "Show curves"
msgstr "显示曲线"

#. Help text for selection boolean for curves
msgctxt "#30014"
msgid "Show Curves.  Default: False"
msgstr "显示曲线。默认：假"
